// Issue Creation and Setup Process
digraph Setup_Workflow {
	dpi=300 fontname=Arial nodesep=0.5 rankdir=TB ranksep=0.75 size="7.5,10" splines=ortho
	node [fontname=Arial fontsize=14 height=0.6 margin="0.2,0.1" shape=box style="rounded,filled"]
	edge [fontname=Arial fontsize=12 penwidth=1.5]
	identify [label="RA identifies task
that needs to be done" fillcolor="#e0eef3"]
	create_issue [label="Create GitHub Issue" fillcolor="#f0f4f8"]
	add_labels [label="Add detailed labels
• Survey ID
• Activity type (missing data, etc.)" fillcolor="#f0f4f8"]
	assign_issue [label="Assign issue to contributor" fillcolor="#f0f4f8"]
	fork [label="New contributor forks
the repository" fillcolor="#e6e0f3"]
	clone [label="Clone fork to
local machine" fillcolor="#e6e0f3"]
	branch [label="Create feature branch
named after issue" fillcolor="#e6e0f3"]
	setup [label="Install required software
• R and RStudio
• Git
• Required system dependencies" fillcolor="#e0eef3"]
	identify -> create_issue
	create_issue -> add_labels
	add_labels -> assign_issue
	assign_issue -> fork
	fork -> clone
	clone -> setup
	setup -> branch
	fontsize=18 label="Phase 1: Issue Creation and Setup Process" labelloc=t
}
